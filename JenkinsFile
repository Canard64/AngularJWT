pipeline {
    agent any

    stages {
         stage('Clone') {
            steps {
                echo 'Cloning..'
                  sh 'rm -rf *'
                sh 'git clone https://github.com/Canard64/AngularJWT'

            }
        }
          stage('Dependencies') {
            steps {
              echo 'PWD'
              sh '$PWD'
              sh 'npm install'
            }
        }
    }
}
