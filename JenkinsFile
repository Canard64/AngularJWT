pipeline {
    agent any

    stages {
         stage('Clone') {
            steps {
                echo 'Cloning..'
                sh 'rm -rf *'
                sh 'git clone https://github.com/Canard64/AngularJWT'

            }
        }
          stage('Build') {
            steps {
                echo 'Building.'
                sh 'cd AngularJWT/'
                sh 'npm run build'

            }
        }
    }
}
