pipeline {
    agent any
    tools {nodejs "node"}

    stages {
         stage('Clone') {
            steps {
                echo 'Cloning..'
                sh 'git clone https://github.com/Canard64/AngularJWT'

            }
        }
          stage('Dependencies') {
            steps {
                sh 'npm install'
               }
        }
        stage('Test') {
            steps {
                sh 'npm test'
               }
        }

    }
}
